/* components/phone-list-item/index.wxss */
.item{
  padding: 10rpx 20rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
}

.phone-image {
  width: 120rpx;
  height: 90rpx;
  
}
.image{
  border-radius: 15rpx;
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.phone-name{
  margin-left: 20rpx;
  font-weight: 600;
  font-size: 28rpx;
}