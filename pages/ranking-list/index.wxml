<!--pages/ranking-list/index.wxml-->
<area-header title="今日热门"  showRight="{{false}}"></area-header>
<view class="ranking">
  <block wx:for="{{ranking}}"  wx:key="spuId">
    <view class="ranking-item">
      <ranking-item item="{{item}}" index="{{index+1}}" bindtap="showPhoneDetail" data-info="{{item}}"></ranking-item>
    </view>
  </block>
</view>
